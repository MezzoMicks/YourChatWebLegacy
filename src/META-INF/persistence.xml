<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.0"
	xmlns="http://java.sun.com/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/persistence http://java.sun.com/xml/ns/persistence/persistence_2_0.xsd">
	<persistence-unit name="YourChatWeb" transaction-type="JTA">
		<provider>org.eclipse.persistence.jpa.PersistenceProvider</provider>
		<jta-data-source>PSQLThemis</jta-data-source>
		<class>de.yovi.chat.persistence.UserEntity</class>
		<class>de.yovi.chat.persistence.PrivateMessageEntity</class>
		<class>de.yovi.chat.persistence.ProfileEntity</class>
		<class>de.yovi.chat.persistence.ImageEntity</class>
		<class>de.yovi.chat.persistence.FriendListEntity</class>
		<class>de.yovi.chat.persistence.FriendListUserEntity</class>
	</persistence-unit>
</persistence>
